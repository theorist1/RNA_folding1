Cell[BoxData[
 RowBox[{"PrintInput", ":=", 
  RowBox[{"Quiet", "@", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"parent", "=", 
        RowBox[{"Extract", "[", 
         RowBox[{
          RowBox[{"DownValues", "[", "In", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "2"}], ",", "2"}], "}"}], ",", "HoldForm"}], "]"}]}],
        ",", 
       RowBox[{"daughter1", "=", 
        RowBox[{"Extract", "[", 
         RowBox[{
          RowBox[{"DownValues", "[", "In", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "2"}], ",", "2", ",", "1"}], "}"}], ",", 
          "HoldForm"}], "]"}]}], ",", 
       RowBox[{"daughter2", "=", 
        RowBox[{"Extract", "[", 
         RowBox[{
          RowBox[{"DownValues", "[", "In", "]"}], ",", 
          RowBox[{"{", 
           RowBox[{
            RowBox[{"-", "2"}], ",", "2", ",", "1"}], "}"}]}], "]"}]}]}], 
      "}"}], ",", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"(", 
         RowBox[{"!", 
          RowBox[{"StringContainsQ", "[", 
           RowBox[{
            RowBox[{"ToString", "@", "daughter1"}], ",", "\"\<==\>\""}], 
           "]"}]}], ")"}], "&&", 
        RowBox[{"(", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"!", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"Depth", "[", "parent", "]"}], "<", "5"}], "&&", 
               RowBox[{"StringContainsQ", "[", 
                RowBox[{
                 RowBox[{"ToString", "@", "parent"}], ",", "\"\<[\>\""}], 
                "]"}]}], ")"}]}], "&&", 
            RowBox[{"(", 
             RowBox[{
              RowBox[{
               RowBox[{"Depth", "[", "parent", "]"}], "<", "5"}], "||", 
              RowBox[{"StringContainsQ", "[", 
               RowBox[{
                RowBox[{"ToString", "@", "parent"}], ",", "\"\<:=\>\""}], 
               "]"}]}], ")"}]}], "||", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"StringMatchQ", "[", 
              RowBox[{
               RowBox[{"ToString", "@", 
                RowBox[{"Extract", "[", 
                 RowBox[{
                  RowBox[{"DownValues", "[", "In", "]"}], ",", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"-", "2"}], ",", "2", ",", "1", ",", "2"}], "}"}],
                   ",", "HoldForm"}], "]"}]}], ",", 
               RowBox[{"ToString", "@", 
                RowBox[{"Extract", "[", 
                 RowBox[{
                  RowBox[{"DownValues", "[", "In", "]"}], ",", 
                  RowBox[{"{", 
                   RowBox[{
                    RowBox[{"-", "2"}], ",", "2", ",", "1", ",", "2"}], 
                   "}"}]}], "]"}]}]}], "]"}], "&&", 
             RowBox[{"StringContainsQ", "[", 
              RowBox[{
               RowBox[{"ToString", "@", "daughter1"}], ",", "\"\<=\>\""}], 
              "]"}]}], ")"}]}], ")"}], ")"}]}], ",", 
       RowBox[{"Print", "[", "daughter1", "]"}], ",", 
       RowBox[{"Print", "[", 
        RowBox[{"daughter1", ",", "\"\< = \>\"", ",", "daughter2"}], "]"}]}], 
      "]"}]}], "]"}]}]}]], "Input",
